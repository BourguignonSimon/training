# Server Configuration
PORT=3001

# Strava OAuth Credentials (Get from https://www.strava.com/settings/api)
STRAVA_CLIENT_ID=your_strava_client_id
STRAVA_CLIENT_SECRET=your_strava_client_secret

# Garmin OAuth Credentials (Optional - for future implementation)
GARMIN_CONSUMER_KEY=your_garmin_consumer_key
GARMIN_CONSUMER_SECRET=your_garmin_consumer_secret

# CORS Configuration (Frontend URL)
CORS_ORIGIN=http://localhost:3000
FRONTEND_URL=http://localhost:3000

# Google Gemini AI API Key (Get from https://aistudio.google.com/app/apikey)
GEMINI_API_KEY=your_gemini_api_key

# Environment
NODE_ENV=development
